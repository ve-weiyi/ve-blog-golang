syntax = "v1"

type (
    OnlineCountResp {
        Msg string `json:"msg"`   // 消息
        Count int `json:"count"`  // 在线人数
    }
)

@server(
    tags : "WebSocket"
    prefix: /admin-api/v1
    group: websocket
    authType: apiKey
    middleware: JwtToken,Permission,OperationLog
)

service admin-api {
    @doc "WebSocket消息"
    @handler Websocket
    get /websocket
}
