syntax = "v1"

import "types.api"

@server(
    prefix: /api/v1
    group: upload
)

service blog-api {
    @doc "上传文件"
    @handler UploadFile
    post /api/v1/upload/:label () returns (UploadRecord)
    
    @doc "上传语言"
    @handler UploadVoice
    post /api/v1/voice () returns (UploadRecord)
    
}
